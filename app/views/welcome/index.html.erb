<h1>Welcome to My Travel Site</h1>
<p>This is my very awesome travel site.</p>

<p><%= @homeland %></p>

<ul>
	<% @countries.each do |country| %>
	<li><%= country %></li>
	<% end %>
</ul>


<p>
<% @images.each do |image| %>
	<%= image_tag image, alt: "Picture Missing", class: "img-responsive" %>
	<% end %>
</p> 

<table class="table">
	<tr>
		<% @united_states.each do |k,v| %>
		<th><%= k %></th>
		<% end %>
	</tr>
	<tr>
		<% @united_states.each do |k,v| %>
		<td><%= v %></td>
		<% end %>
	</tr>
</table>

